programa {
  funcao inicio() {

    inteiro numero

    escreva("Digite um número: ")
    leia(numero)
    
    se (numero == 10) {
      escreva("É IGUAL A 10.")
    } senao se (numero < 10) {
     escreva("É MENOR QUE 10!")
    } senao {
     escreva("É MAIOR QUE 10!")
    }
 
    escreva("=== Fim do programa> ===")
  }
}
